<% if logged_in? %>

<h1>Import File for Geocoding</h1>
<%= form_tag import_locations_path, multipart: true, remote: true, id:'fileupload'  do %>

  <%= file_field_tag :file %>
  <%= submit_tag "GO!", id:'content' %>
<%= token_tag form_authenticity_token %>
<% end %>



